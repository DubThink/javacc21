/*
 * A JSONC (JSON with comments) grammar 
 */
PARSER_PACKAGE=org.parsers.jsonc;
PARSER_CLASS=JSONCParser;
DEFAULT_LEXICAL_STATE=JSON;
FAULT_TOLERANT;
DEBUG_FAULT_TOLERANT;

UNPARSED #Comment :
    <SINGLE_LINE_COMMENT : "//" (~["\n","\r"])*  ("\n"|"\r"|"\r\n") >
    |
    <MULTI_LINE_COMMENT: "/*" (~["*"])* "*" ("*" | (~["*","/"] (~["*"])* "*"))* "/">
;

INCLUDE "/include/json/JSON.javacc"
